generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model LastHeartbeat {
  id                Int      @id @default(autoincrement())
  heartbeatCreatedAt DateTime @map("heartbeat_created_at")
  updatedAt         DateTime @updatedAt
}

